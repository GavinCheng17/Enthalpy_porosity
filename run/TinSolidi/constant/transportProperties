FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      transportProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

transportModel Newtonian;

// Laminar viscosity
nu              [0 2 -1 0 0 0 0] 8e-07;

// Thermal expansion coefficient
beta            [0 0 0 -1 0 0 0] 2.67e-04;

// Reference temperature
TRef            [0 0 0 1 0 0 0] 505;

// Laminar Prandtl number
Pr              [0 0 0 0 0 0 0] 0.02;

// Turbulent Prandtl number
Prt             [0 0 0 0 0 0 0] 1.0;

// ************************************************************************* //

//Latent heat of fusion
L_heat          [0 2 -2 0 0 0 0] 6e4;

//Specific heat capacity
cp              [0 2 -2 -1 0 0 0] 200;

//Mushy zone constant
C_mushy         [1 -3 -1 0 0 0 0] 1e5;

//Computational constant to avoid division by zero
eps_poro        [0 0 0 0 0 0 0] 1e-03;

//reference density kg/m^3
rho_ref         [1 -3 0 0 0 0 0] 7500;

//relaxation factor in the enthalpy update equation
omega_DH        [0 0 0 0 0 0 0] 0.3;



