FoamFile
{
    version     2.0;
    format      binary;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    top
    {
        type            noSlip;
    }
    bott
    {
        type            noSlip;
    }
    left
    {
        type            noSlip;
    }
    right
    {
        type            noSlip;
    }
    frontAndBack
    {
        type            empty;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


